---
interface Props {
    dark?: boolean;
}

const { dark } = Astro.props;
---
<div class="flex justify-evenly items-center gap-2">
    <a href="#"> <img class='h-[4.5rem] w-[4.5rem]' src=`/svgs/${dark? 'dark' : 'white'}/uniswap.svg` alt="uniswap"> </a>

    <a href="#"> <img class='h-16 w-16' src=`/svgs/${dark? 'dark' : 'white'}/dexscreener.svg` alt="dexscreener"> </a>

    <a href="#"> <img class='h-16 w-16' src=`/svgs/${dark? 'dark' : 'white'}/dextools.svg` alt="dextools"> </a>

    <a href="#"> <img class='h-16 w-16 ml-1' src=`/svgs/${dark? 'dark' : 'white'}/telegram.svg` alt="telegram"> </a>

    <a href="#"> <img class='h-20 w-20 -ml-1' src=`/svgs/${dark? 'dark' : 'white'}/x.svg` alt="x"> </a>
</div>
